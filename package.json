{
  "name": "webpack-vue-mpa",
  "description": "项目描述",
  "author": "Shinvey Hu <shinvey@qq.com>",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "dev:local": "cross-env WSAPI_ENV=local webpack-dev-server --inline --config build/webpack.dev.conf.js",
    "dev": "cross-env WSAPI_ENV=development webpack-dev-server --inline --config build/webpack.dev.conf.js",
    "dev:staging": "cross-env WSAPI_ENV=staging webpack-dev-server --inline --config build/webpack.dev.conf.js",
    "dev:open": "cross-env WSAPI_ENV=development webpack-dev-server --open --inline --config build/webpack.dev.conf.js",
    "build:local": "cross-env WSAPI_ENV=local webpack --mode development --debug --config build/webpack.prod.conf.js",
    "build:dev": "cross-env WSAPI_ENV=development webpack --mode development --debug --config build/webpack.prod.conf.js",
    "build:staging": "cross-env WSAPI_ENV=staging webpack --mode development --debug --config build/webpack.prod.conf.js",
    "build:staging:zip": "cross-env WSAPI_ENV=staging webpack --mode development --zip --debug --config build/webpack.prod.conf.js",
    "build:staging:minify": "cross-env WSAPI_ENV=staging webpack --mode production --config build/webpack.prod.conf.js",
    "build:staging:minify:zip": "cross-env WSAPI_ENV=staging webpack --mode production --zip --config build/webpack.prod.conf.js",
    "build:prd": "webpack --mode production --config build/webpack.prod.conf.js",
    "build:prd:zip": "webpack --mode production --zip --config build/webpack.prod.conf.js",
    "build:profile": "webpack --profile --progress --colors --debug --verbose --config build/webpack.prod.conf.js",
    "mock": "node mock/server.js",
    "corsproxy": "cross-env PORT=8082 node corsproxy.js",
    "lint": "eslint --ext .js,.vue src"
  },
  "dependencies": {
    "@antv/f2": "^3.2.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime-corejs2": "^7.0.0",
    "axios": "^0.18.0",
    "element-ui": "^1.2.8",
    "highcharts": "^6.1.3",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "textfit": "git+https://github.com/STRML/textFit.git",
    "vue": "^2.5.16",
    "vux": "^2.9.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
